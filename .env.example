# Freqtrade Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control!

# Bybit API Configuration
BYBIT_API_KEY=your_api_key_here
BYBIT_API_SECRET=your_api_secret_here

# Telegram Bot Configuration (optional)
TELEGRAM_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_chat_id

# FreqUI Configuration
FREQTRADE_UI_USERNAME=admin
FREQTRADE_UI_PASSWORD=your_secure_password

# Database URL (optional - for custom database)
DATABASE_URL=sqlite:///user_data/tradesv3.sqlite

# Trading Configuration
DRY_RUN=true
STAKE_AMOUNT=20
MAX_OPEN_TRADES=3

# Security
JWT_SECRET_KEY=your_random_jwt_secret_key_here